{
  "permissions": {
    "allow": [
      "Bash(ls:*)",
      "Bash(pwd:*)",
      "Bash(cat:*)",
      "Bash(grep:*)",
      "Bash(find:*)",
      "Bash(git log:*)",
      "Bash(git diff:*)",
      "Bash(gh pr:*)",
      "Bash(gh issue:*)",
      "Read(~/obsidian-vaults/**)",
      "Edit(~/obsidian-vaults/**)"
    ],
    "deny": [
      "Read(~/obsidian-vaults/.obsidian/**)",
      "Edit(~/obsidian-vaults/.obsidian/**)"
    ]
  },
  "enabledPlugins": {
    "code-review@claude-plugins-official": true,
    "github@claude-plugins-official": true
  }
}
